{
  "name": "ping-monitor",
  "version": "1.0.0",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "ping": "^0.2.2"
  },
  "pkg": {
    "scripts": "server.js",
    "assets": [
      "public/**/*"
